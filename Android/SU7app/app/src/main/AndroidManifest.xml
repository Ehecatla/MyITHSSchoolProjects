<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tfk16eei.su7app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleDrawActivity"
            android:label="@string/draw_activity_label" />
        <activity
            android:name=".InteractionActivity"
            android:label="@string/interaction_activity_label" />
        <activity
            android:name=".AnimationActivity"
            android:label="@string/animation_activity_label" />
        <activity android:name=".OwnActivity"
            android:label="@string/own_activity_label"/>
    </application>

</manifest>